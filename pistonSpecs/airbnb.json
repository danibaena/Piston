{
  "info": {
    "title": "AirBnB",
    "description": "API to retrieve different info from AirBnB",
    "version": "1.0",
    "author": "h4ckademy"
  },
  "defaults": {
    "baseUrl": "https://api.airbnb.com"
  },
  "action": [{
    "name": "getUser",
    "uri": "https://api.airbnb.com/v2/users/{userID}",
    "method": "GET",
    "qs": {
      "client_id": "3092nxybyb0otqw18e8nh5nty"
    },
    "headers": {
      "host": "api.airbnb.com",
      "User-Agent": "Airbnb/160610 Android/4.0.4 Device/Sony_LT15i Carrier/Movistar Type/Phone",
      "Connection": "keep-alive"
    },
    "arguments": [
      "userID"
    ],
    "after": null,
    "extract": [
      "user.first_name",
      "user.picture_url"
    ]
  }, {
    "name": "login",
    "uri": "https://api.airbnb.com/v1/authorize",
    "method": "POST",
    "qs": {
      "client_id": "3092nxybyb0otqw18e8nh5nty"
    },
    "headers": {
      "content-type": "application/x-www-form-urlencoded",
      "cache-control": "no-cache",
      "User-Agent": "Airbnb/160610 Android/4.0.4 Device/Sony_LT15i Carrier/Movistar Type/Phone"
    },
    "form": {
      "grant_type": "password",
      "password": "{pass}",
      "username": "{username}"
    },
    "arguments": [
      "username",
      "pass"
    ],
    "after": "token",
    "extract": [
      "access_token"
    ]
  }]
}
